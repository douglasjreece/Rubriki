@page "/contestant/{id}"
@using Rubriki.SharedComponents

<PageTitle>Contestant</PageTitle>

<AuthorizeView>
    <Authorized>
        <ContestantPanel ContestantId="@Id" />
    </Authorized>
    <NotAuthorized>
        <SignInRequiredMessage />
    </NotAuthorized>
</AuthorizeView>


@code {
    [Parameter]
    public string? Id { get; set; }
}
